{"ast":null,"code":"import HeaderComponent from \"@/components/Header.vue\";\nimport NavigationComponent from \"./components/NavigationDrawer.vue\";\nimport MainComponent from \"@/components/Main.vue\";\nimport FooterComponent from \"@/components/Footer.vue\";\nimport store from \"@/store\";\nexport default {\n  name: \"App\",\n  components: {\n    HeaderComponent,\n    MainComponent,\n    FooterComponent,\n    NavigationComponent\n  },\n  computed() {\n    if (this.vuetify.display.width > 992) {\n      store.dispatch(\"updateDrawer\", {\n        drawer: !this.drawer\n      });\n    }\n  }\n};","map":{"version":3,"names":["HeaderComponent","NavigationComponent","MainComponent","FooterComponent","store","name","components","computed","vuetify","display","width","dispatch","drawer"],"sources":["/home/sabi/Desktop/ToDo/frontend/todo/src/App.vue"],"sourcesContent":["<template>\n  <v-app>\n    <div class=\"container\">\n      <nav><HeaderComponent /></nav>\n      <aside><NavigationComponent /></aside>\n      <main><MainComponent /></main>\n      <footer><FooterComponent /></footer>\n    </div>\n  </v-app>\n</template>\n\n<script>\nimport HeaderComponent from \"@/components/Header.vue\";\nimport NavigationComponent from \"./components/NavigationDrawer.vue\";\nimport MainComponent from \"@/components/Main.vue\";\nimport FooterComponent from \"@/components/Footer.vue\";\nimport store from \"@/store\";\n\nexport default {\n  name: \"App\",\n  components: {\n    HeaderComponent,\n    MainComponent,\n    FooterComponent,\n    NavigationComponent,\n  },\n  computed() {\n    if (this.vuetify.display.width > 992) {\n      store.dispatch(\"updateDrawer\", {\n        drawer: !this.drawer,\n      });\n    }\n  },\n};\n</script>\n<style>\nbody {\n  padding: 0px;\n  margin: 0px;\n}\nhtml {\n  scrollbar-width: none;\n}\nnav {\n  grid-area: header;\n  z-index: 9999;\n}\naside {\n  grid-area: sidebar;\n}\nmain {\n  grid-area: main;\n}\nfooter {\n  grid-area: footer;\n}\n@media screen and