{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from \"axios\";\nimport BarChart from \"@/components/BarChart.vue\";\nimport { Doughnut } from \"vue-chartjs\";\nimport { Chart as ChartJS, ArcElement, Tooltip, Legend } from \"chart.js\";\nChartJS.register(ArcElement, Tooltip, Legend);\nexport default {\n  name: \"StatisticView\",\n  components: {\n    BarChart,\n    Doughnut\n  },\n  data() {\n    return {\n      USERS_LIST_URL: \"http://192.168.1.115:8000/auth/users/all/\",\n      usersList: null,\n      usernames: [],\n      taskSum: [],\n      userStats: {\n        completed: null,\n        notCompleted: null\n      },\n      data: {\n        labels: this.usernames,\n        datasets: [{\n          backgroundColor: [\"#1abc9c\", \"#9b59b6\", \"#d35400\", \"#34495e\", \"#f1c40f\", \"#3498db\", \"#c0392b\", \"#16a085\", \"#f39c12\", \"#2c3e50\", \"#e74c3c\"],\n          data: this.taskSum\n        }]\n      },\n      options: {\n        responsive: true,\n        maintainAspectRatio: false,\n        plugins: {\n          legend: {\n            position: \"bottom\"\n          }\n        }\n      }\n    };\n  },\n  methods: {\n    async getUsers() {\n      try {\n        const response = await axios.get(this.USERS_LIST_URL);\n        if (response.status == 200) {\n          this.usersList = response.data;\n          for (let i = 0; i < this.usersList.length; i++) {\n            this.usernames.push(this.usersList[i].username);\n            const taskSum = this.usersList[i].completed + this.usersList[i].not_completed;\n            if (taskSum == 0) {\n              this.taskSum.push(1);\n            } else {\n              this.taskSum.push(taskSum);\n            }\n          }\n        }\n      } catch (err) {\n        alert(\"90\");\n      }\n    }\n  },\n  mounted() {\n    this.getUsers();\n  }\n};","map":{"version":3,"names":["axios","BarChart","Doughnut","Chart","ChartJS","ArcElement","Tooltip","Legend","register","name","components",