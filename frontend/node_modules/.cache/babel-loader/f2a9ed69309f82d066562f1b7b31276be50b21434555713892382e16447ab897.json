 },\n        ],\n      },\n      options: {\n        responsive: true,\n        maintainAspectRatio: false,\n        plugins: {\n          legend: {\n            position: \"bottom\",\n          },\n        },\n      },\n    };\n  },\n  methods: {\n    async getUsers() {\n      try {\n        const response = await axios.get(this.USERS_LIST_URL);\n        if (response.status == 200) {\n          this.usersList = response.data;\n          for (let i = 0; i < this.usersList.length; i++) {\n            this.usernames.push(this.usersList[i].username);\n            const taskSum =\n              this.usersList[i].completed + this.usersList[i].not_completed;\n            if (taskSum == 0) {\n              this.taskSum.push(1);\n            } else {\n              this.taskSum.push(taskSum);\n            }\n          }\n        }\n      } catch (err) {\n        alert(\"90\");\n      }\n    },\n  },\n  mounted() {\n    this.getUsers();\n  },\n};\n</script>\n<style></style>\n"],"mappings":";AAkBA,OAAOA,KAAI,MAAO,OAAO;AACzB,OAAOC,QAAO,MAAO,2BAA2B;AAChD,SAASC,QAAO,QAAS,aAAa;AACtC,SAASC,KAAI,IAAKC,OAAO,EAAEC,UAAU,EAAEC,OAAO,EAAEC,MAAK,QAAS,UAAU;AAExEH,OAAO,CAACI,QAAQ,CAACH,UAAU,EAAEC,OAAO,EAAEC,MAAM,CAAC;AAE7C,eAAe;EACbE,IAAI,EAAE,eAAe;EACrBC,UAAU,EAAE;IACVT,QAAQ;IACRC;EACF,CAAC;EACDS,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,cAAc,EAAE,2CAA2C;MAC3DC,SAAS,EAAE,IAAI;MACfC,SAAS,EAAE,EAAE;MACbC,OAAO,EAAE,EAAE;MACXC,SAAS,EAAE;QACTC,SAAS,EAAE,IAAI;QACfC,YAAY,EAAE;MAChB,CAAC;MACDP,IAAI,EAAE;QACJQ,MAAM,EAAE,IAAI,CAACL,SAAS;QACtBM,QAAQ,EAAE,CACR;UACEC,eAAe,EAAE,CACf,SAAS,EACT,SAAS,EACT,SAAS,EACT,SAAS,EACT,SAAS,EACT,SAAS,EACT,SAAS,EACT,SAAS,EACT,SAAS,EACT,SAAS,EACT,SAAS,CACV;UACDV,IAAI,EAAE,IAAI,CAACI;QACb,CAAC;MAEL,CAAC;MACDO,OAAO,EAAE;QACPC,UAAU,EAAE,IAAI;QAChBC,mBAAmB,EAAE,KAAK;QAC1BC,OAAO,EAAE;UACPC,MAAM,EAAE;YACNC,QAAQ,EAAE;UACZ;QACF;MACF;IACF,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,QAAQA,CAAA,EAAG;MACf,IAAI;QACF,MAAMC,QAAO,GAAI,MAAM9B,KAAK,CAAC+B,GAAG,CAAC,IAAI,CAACnB,cAAc,CAAC;QACrD,IAAIk