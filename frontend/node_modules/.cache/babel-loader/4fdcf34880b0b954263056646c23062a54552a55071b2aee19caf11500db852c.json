BarChart>\n    </div>\n  </div>\n  <div>\n    <DoughnutChart :usernames=\"usernames\" :taskSum=\"taskSum\"></DoughnutChart>\n  </div>\n</template>\n<script>\nimport axios from \"axios\";\nimport BarChart from \"@/components/BarChart.vue\";\nimport DoughnutChart from \"@/components/DoughnutChart.vue\";\n\nexport default {\n  name: \"StatisticView\",\n  components: {\n    BarChart,\n    DoughnutChart,\n  },\n  data() {\n    return {\n      USERS_LIST_URL: \"http://192.168.1.115:8000/auth/users/all/\",\n      usersList: null,\n      userStats: {\n        completed: null,\n        notCompleted: null,\n      },\n    };\n  },\n  methods: {\n    getUsers() {\n      try {\n        axios.get(this.USERS_LIST_URL).then((response) => {\n          if (response.status == 200) {\n            this.usersList = response.data;\n          }\n        });\n      } catch (err) {\n        alert(\"90\");\n      }\n    },\n  },\n  mounted() {\n    this.getUsers();\n  },\n  computed: {\n    usernames() {\n      const usernames = [];\n      for (let i = 0; i < this.usersList.length; i++) {\n        usernames.push(this.usersList[i].username);\n      }\n      return usernames;\n    },\n    taskSum() {\n      const taskSum = [];\n      for (let i = 0; i < this.usersList.length; i++) {\n        taskSum.push(\n          this.usersList[i].completed + this.usersList[i].not_completed\n        );\n      }\n      return taskSum;\n    },\n  },\n};\n</script>\n<style></style>\n"],"mappings":";AAeA,OAAOA,KAAI,MAAO,OAAO;AACzB,OAAOC,QAAO,MAAO,2BAA2B;AAChD,OAAOC,aAAY,MAAO,gCAAgC;AAE1D,eAAe;EACbC,IAAI,EAAE,eAAe;EACrBC,UAAU,EAAE;IACVH,QAAQ;IACRC;EACF,CAAC;EACDG,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,cAAc,EAAE,2CAA2C;MAC3DC,SAAS,EAAE,IAAI;MACfC,SAAS,EAAE;QACTC,SAAS,EAAE,IAAI;QACfC,YAAY,EAAE;MAChB;IACF,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,QAAQA,CAAA,EAAG;MACT,IAAI;QACFZ,KAAK,CAACa,GAAG,CAAC,IAAI,CAACP,cAAc,CAAC,CAACQ,IAAI,CAAEC,QAAQ,IAAK;UAChD,IAAIA,QAAQ,CAACC,MAAK,IAAK,GAAG,EAAE;YAC1B,IAAI,CAACT,SAAQ,GAAIQ,QAAQ,CAACV,I