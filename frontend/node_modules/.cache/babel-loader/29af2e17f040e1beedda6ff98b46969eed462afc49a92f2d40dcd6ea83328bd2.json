 <div>{{ usersList }}</div>\n  <div>{{ usernames }}</div>\n  <div>{{ taskSum }}</div>\n</template>\n<script>\nimport { Chart as ChartJS, ArcElement, Tooltip, Legend } from \"chart.js\";\nimport { Doughnut } from \"vue-chartjs\";\nChartJS.register(ArcElement, Tooltip, Legend);\n\nexport default {\n  name: \"DoughnutChart\",\n  components: {\n    Doughnut,\n  },\n  props: {\n    usersList: Array,\n  },\n  data() {\n    return {\n      usernames: [],\n      taskSum: [],\n      data: {\n        labels: [\"this.usernames\"],\n        datasets: [\n          {\n            backgroundColor: [\n              \"#1abc9c\",\n              \"#9b59b6\",\n              \"#d35400\",\n              \"#34495e\",\n              \"#f1c40f\",\n              \"#3498db\",\n              \"#c0392b\",\n              \"#16a085\",\n              \"#f39c12\",\n              \"#2c3e50\",\n              \"#e74c3c\",\n            ],\n            data: [90, 90],\n          },\n        ],\n      },\n      options: {\n        responsive: true,\n        maintainAspectRatio: false,\n        plugins: {\n          legend: {\n            position: \"bottom\",\n          },\n        },\n      },\n    };\n  },\n  methods: {\n    getUsernames() {\n      for (let i = 0; i < this.usersList.length; i++) {\n        this.usernames.push(this.usersList[i].username);\n      }\n    },\n    getTaskSum() {\n      for (let i = 0; i < this.usersList.length; i++) {\n        this.taskSum.push(\n          this.usersList[i].completed + this.usersList[i].not_completed\n        );\n      }\n    },\n  },\n  mounted() {\n    this.getUsernames();\n    this.getTaskSum();\n  },\n};\n</script>\n<style>\ncanvas {\n  width: auto;\n  height: auto;\n}\n</style>\n"],"mappings":";AASA,SAASA,KAAI,IAAKC,OAAO,EAAEC,UAAU,EAAEC,OAAO,EAAEC,MAAK,QAAS,UAAU;AACxE,SAASC,QAAO,QAAS,aAAa;AACtCJ,OAAO,CAACK,QAAQ,CAACJ,UAAU,EAAEC,OAAO,EAAEC,MAAM,CAAC;AAE7C,eAAe;EACbG,IAAI,EAAE,eAAe;EACrBC,UAAU,EAAE;IACVH;EACF,CAAC;EACDI,KAAK,EAAE;IACLC,SAAS,EAAEC;EACb,C