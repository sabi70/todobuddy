{"ast":null,"code":"import { useTheme } from \"vuetify\";\nimport NavDrawer from \"@/components/NavDrawer.vue\";\nimport { mapActions, mapState } from \"vuex\";\nimport store from \"@/store\";\nexport default {\n  name: \"AppBar\",\n  components: {\n    NavDrawer\n  },\n  data() {\n    return {\n      theme: useTheme(),\n      darkMode: false\n    };\n  },\n  methods: {\n    changeTheme() {\n      this.darkMode = !this.darkMode;\n      this.theme.global.name = this.darkMode ? \"darkTheme\" : \"lightTheme\";\n    },\n    openDrawer() {\n      this.$refs.navDrawer.openDrawer();\n    }\n  },\n  computed: {\n    ...mapState([\"isAuthenticated\", \"user\"]),\n    ...mapActions([\"updateUser\"])\n  },\n  created() {\n    this.updateUser(user = \"lolo\", isAuthenticated = true);\n  }\n};","map":{"version":3,"names":["useTheme","NavDrawer","mapActions","mapState","store","name","components","data","theme","darkMode","methods","changeTheme","global","openDrawer","$refs","navDrawer","computed","created","updateUser","user","isAuthenticated"],"sources":["/home/sabi/Desktop/ToDo/frontend/todo/src/components/AppBar.vue"],"sourcesContent":["<template>\n  <v-app-bar :elevation=\"2\">\n    <template v-slot:prepend>\n      <v-app-bar-nav-icon\n        @click=\"openDrawer\"\n        icon=\"mdi-menu\"\n      ></v-app-bar-nav-icon>\n    </template>\n    <v-app-bar-title class=\"text-center\">\n      <p v-if=\"isAuthenticated\">lolololo</p>\n    </v-app-bar-title>\n    <template v-slot:append>\n      <v-btn\n        :icon=\"darkMode ? 'mdi-weather-night' : 'mdi-white-balance-sunny'\"\n        :color=\"darkMode ? 'primary' : 'warning'\"\n        @click=\"changeTheme\"\n      ></v-btn>\n    </template>\n  </v-app-bar>\n  <NavDrawer ref=\"navDrawer\"></NavDrawer>\n</template>\n<script>\nimport { useTheme } from \"vuetify\";\nimport NavDrawer from \"@/components/NavDrawer.vue\";\nimport { mapActions, mapState } from \"vuex\";\nimport store from \"@/store\";\n\nexport default {\n  name: \"AppBar\",\n  components: {\n    NavDrawer,\n  },\n  data() {\n    return {\n      theme: useTheme(),\n      darkMode: false,\n    };\n  },\n  methods: {\n    changeTheme() {\n      this.darkMode = !this.darkMode;\n      this.theme.global.name = this.darkMode ? \"darkTheme\" : \"lightTheme\";\n    },\n    openDrawer() {\n      this.$refs.navDrawer.openDrawer();\n    },\n  },\n  computed: {\n    ...mapState([\"isAuthenticated\", \"user\"]),\n    ...mapActions([\"updateUser\"]),\n  },\n  created() {\n    this.updateUser(user=\"lolo\", isAuthenticated=true);\n  },\n};\n</script>\n"],"mappings":"AAsBA,SAASA,QAAO,QAAS,SAAS;AAClC,OAAOC,SAAQ,MAAO,4BAA4B;AAClD,SAASC,UAAU,EAAEC,QAAO,QAAS,MAAM;AAC3C,OAAOC,KAAI,MAAO,SAAS;AAE3B,eAAe;EACbC,IAAI,EAAE,QAAQ;EACdC,UAAU,EAAE;IACVL;EACF,CAAC;EACDM,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAER,QAAQ,CAAC,CAAC;MACjBS,QAAQ,EAAE;IACZ,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,WAAWA,CAAA,EAAG;MACZ,IAAI,CAACF,QAAO,GAAI,CAAC,IAAI,CAACA,QAAQ;MAC9B,IAAI,CAACD,KAAK,CAACI,MAAM,CAACP,IAAG,GAAI,IAAI,CAACI,QAAO,GAAI,WAAU,GAAI,YAAY;IACrE,CAAC;IACDI,UAAUA,CAAA,EAAG;MACX,IAAI,CAACC,KAAK,CAACC,SAAS,CAACF,UAAU,CAAC,CAAC;IACnC;EACF,CAAC;EACDG,QAAQ,EAAE;IACR,GAAGb,QAAQ,CAAC,CAAC,iBAAiB,EAAE,MAAM,CAAC,CAAC;IACxC,GAAGD,UAAU,CAAC,CAAC,YAAY,CAAC;EAC9B,CAAC;EACDe,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,UAAU,CAACC,IAAI,GAAC,MAAM,EAAEC,eAAe,GAAC,IAAI,CAAC;EACpD;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}