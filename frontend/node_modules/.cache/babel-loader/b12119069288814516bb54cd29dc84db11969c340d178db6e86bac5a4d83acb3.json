is set to: [AllowAny]\n    await this.getUsers();\n  },\n};\n</script>\n<style scoped>\n@media screen and (max-width: 600px) {\n  #sidebar {\n    position: fixed;\n    top: 64px;\n    width: 100%;\n  }\n}\n@media screen and (max-width: 768px) {\n  #sidebar {\n    position: fixed;\n    top: 64px;\n    width: 100%;\n  }\n}\n@media screen and (min-width: 992px) {\n  #sidebar {\n    position: fixed;\n    top: 64px;\n    width: 250px;\n    height: 100%;\n    padding-bottom: 50px;\n  }\n}\n</style>\n"],"mappings":";AAkDA,SAASA,QAAO,QAAS,MAAM;AAC/B,OAAOC,KAAI,MAAO,SAAS;AAC3B,OAAOC,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAI,EAAE,qBAAqB;EAC3BC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,cAAc,EAAE,2CAA2C;MAC3DC,SAAS,EAAE,IAAI;MACfC,OAAO,EAAE,IAAI;MACbC,YAAY,EAAE,CACZ;QAAEC,KAAK,EAAE,MAAM;QAAEC,GAAG,EAAE,GAAG;QAAEC,IAAI,EAAE;MAAmB,CAAC,EACrD;QAAEF,KAAK,EAAE,UAAU;QAAEC,GAAG,EAAE,WAAW;QAAEC,IAAI,EAAE;MAAkB,CAAC,EAChE;QAAEF,KAAK,EAAE,UAAU;QAAEC,GAAG,EAAE,WAAW;QAAEC,IAAI,EAAE;MAAmB,CAAC,EACjE;QAAEF,KAAK,EAAE,OAAO;QAAEC,GAAG,EAAE,QAAQ;QAAEC,IAAI,EAAE;MAAY,CAAC,EACpD;QAAEF,KAAK,EAAE,OAAO;QAAEC,GAAG,EAAE,QAAQ;QAAEC,IAAI,EAAE;MAA0B,CAAC;IAEtE,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACP;IACAC,aAAaA,CAAA,EAAG;MACdZ,KAAK,CAACa,QAAQ,CAAC,cAAc,EAAE;QAC7BC,MAAM,EAAE,CAAC,IAAI,CAACA;MAChB,CAAC,CAAC;IACJ,CAAC;IACDC,YAAYA,CAAA,EAAG;MACb,IAAI,IAAI,CAACC,WAAU,GAAI,GAAG,EAAE;QAC1B,IAAI;MACN,OAAO;QACL,IAAI,CAACJ,aAAa,CAAC,CAAC;MACtB;IACF,CAAC;IACD;IACA,MAAMK,QAAQA,CAAA,EAAG;MACf,IAAI;QACF,MAAMC,QAAO,GAAI,MAAMjB,KAAK,CAACkB,GAAG,CAAC,IAAI,CAACf,cAAc,CAAC;QACrD,IAAIc,QAAQ,CAACE,MAAK,IAAK,GAAG,EAAE;UAC1B,IAAI,CAACf,SAAQ,GAAIa,QAAQ,CAACf,IAAI;QAChC;MACF,EAAE,OAAOkB,GAAG,EAAE;QACZC,OAAO,CAACC,GAAG,CAACF,GAAG,CAAC;MAClB;IACF,CAAC;IACD;IACAG,QAAQA,CAACC,QAAQ,EAAE;MACjB,IAAI,CAACC,OAAO,CAACC,IAAI,CAAC;QAAEzB,IAAI,EAAE,WAAW;QAAE0B,MAAM,EAAE;UAAEH;QAAS;MAAE,CAAC,CAAC;IAChE;EACF,CAAC;EACDI,QAAQ,EAAE;IACR;IACA,GAAG9B,QAAQ,CAAC,CAAC,QAAQ,CAAC,CAAC;IACvBiB,WAAWA,CAAA,EAAG;MACZ,OAAO,IAAI,CAACc,QAAQ,CAACC,OAAO,CAACC,KAAK;IACpC;EACF,CAAC;E