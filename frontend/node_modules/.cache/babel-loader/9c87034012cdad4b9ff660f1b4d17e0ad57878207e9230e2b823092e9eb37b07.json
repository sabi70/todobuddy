 {\n        this.updateSidebar();\n      }\n    },\n    /* Getting users list from backend */\n    async getUsers() {\n      try {\n        const response = await axios.get(this.USERS_LIST_URL);\n        if (response.status == 200) {\n          this.usersList = response.data;\n        }\n      } catch (err) {\n        console.log(err);\n      }\n    },\n    /* Function that is allow us to route between users statistic pages */\n    goToUser(username) {\n      this.$router.push({ name: \"statistic\", params: { username } });\n    },\n  },\n  computed: {\n    /* Mapping the  data from vuex storage */\n    ...mapState([\"drawer\"]),\n    windowWidth() {\n      return this.$vuetify.display.width;\n    },\n  },\n  async mounted() {\n    await this.getUsers();\n  },\n};\n</script>\n<style scoped>\n@media screen and (max-width: 600px) {\n  #sidebar {\n    position: fixed;\n    top: 64px;\n    width: 100%;\n  }\n}\n@media screen and (max-width: 768px) {\n  #sidebar {\n    position: fixed;\n    top: 64px;\n    width: 100%;\n  }\n}\n@media screen and (min-width: 992px) {\n  #sidebar {\n    position: fixed;\n    top: 64px;\n    width: 250px;\n    height: 100%;\n    padding-bottom: 50px;\n  }\n}\n</style>\n"],"mappings":";AAiDA,SAASA,QAAO,QAAS,SAAS;AAClC,SAASC,QAAO,QAAS,MAAM;AAC/B,OAAOC,KAAI,MAAO,SAAS;AAC3B,OAAOC,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAI,EAAE,qBAAqB;EAC3BC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,cAAc,EAAE,2CAA2C;MAC3DC,SAAS,EAAE,IAAI;MACfC,OAAO,EAAE,IAAI;MACbC,KAAK,EAAET,QAAQ,CAAC,CAAC;MACjBU,QAAQ,EAAE,KAAK;MACfC,YAAY,EAAE,CACZ;QAAEC,KAAK,EAAE,MAAM;QAAEC,GAAG,EAAE,GAAG;QAAEC,IAAI,EAAE;MAAmB,CAAC,EACrD;QAAEF,KAAK,EAAE,UAAU;QAAEC,GAAG,EAAE,UAAU;QAAEC,IAAI,EAAE;MAAkB,CAAC,EAC/D;QAAEF,KAAK,EAAE,UAAU;QAAEC,GAAG,EAAE,UAAU;QAAEC,IAAI,EAAE;MAAmB,CAAC,EAChE;QAAEF,KAAK,EAAE,OAAO;QAAEC,GAAG,EAAE,OAAO;QAAEC,IAAI,EAAE;MAAY,CAAC,EACnD;QAAEF,KAAK,EAAE,WAAW;QAAEC,GAAG,EAAE,YAAY;QAAEC,IAAI,EAAE;MAAe,CAAC,EAC/D;QAAEF,KAAK,EAAE,OAAO;QAAEC,GAAG,EAAE,OAAO;QAAEC,IAAI,EAAE;MAA0B,CAAC;IAErE,CAAC;E