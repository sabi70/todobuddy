{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { Chart as ChartJS, ArcElement, Tooltip, Legend } from \"chart.js\";\nimport { Doughnut } from \"vue-chartjs\";\nChartJS.register(ArcElement, Tooltip, Legend);\nexport default {\n  name: \"DoughnutChart\",\n  components: {\n    Doughnut\n  },\n  props: {\n    usersList: Array\n  },\n  data() {\n    return {\n      taskSum: [],\n      data: {\n        labels: this.computed.usernames,\n        datasets: [{\n          backgroundColor: [\"#1abc9c\", \"#9b59b6\", \"#d35400\", \"#34495e\", \"#f1c40f\", \"#3498db\", \"#c0392b\", \"#16a085\", \"#f39c12\", \"#2c3e50\", \"#e74c3c\"],\n          data: [90, 90]\n        }]\n      },\n      options: {\n        responsive: true,\n        maintainAspectRatio: false,\n        plugins: {\n          legend: {\n            position: \"bottom\"\n          }\n        }\n      }\n    };\n  },\n  methods: {\n    getUsernames() {\n      for (let i = 0; i < this.usersList.length; i++) {\n        this.usernames.push(this.usersList[i].username);\n      }\n    },\n    getTaskSum() {\n      for (let i = 0; i < this.usersList.length; i++) {\n        this.taskSum.push(this.usersList[i].completed + this.usersList[i].not_completed);\n      }\n    }\n  },\n  computed: {\n    usernames() {\n      return this.usersList.map(user => user.username);\n    }\n  }\n};","map":{"version":3,"names":["Chart","ChartJS","ArcElement","Tooltip","Legend","Doughnut","register","name","components","props","usersList","Array","data","taskSum","labels","computed","usernames","datasets","backgroundColor","options","responsive","maintainAspectRatio","plugins","legend","position","methods","getUsernames","i","length","push","username","getTaskSum","completed","not_completed","map","user"],"sources":["/home/sabi/Desktop/ToDo/frontend/todo/src/components/DoughnutChart.vue"],"sourcesContent":["<template>\n  <div class=\"d-flex align-center justify-center\">\n    <Doughnut :data=\"data\" :options=\"options\"></Doughnut>\n  </div>\n  <div>{{ usernames }}</div>\n</template>\n<script>\nimport { Chart as ChartJS, ArcElement, Tooltip, Legend } from \"chart.js\";\nimport { Doughnut } from \"vue-chartjs\";\nChartJS.register(ArcElement, Tooltip, Legend);\n\nexport default {\n  name: \"DoughnutChart\",\n  components: {\n    Doughnut,\n  },\n  props: {\n    usersList: Array,\n  },\n  data() {\n    return {\n      taskSum: [],\n      data: {\n        labels: this.computed.usernames,\n        datasets: [\n          {\n            backgroundColor: [\n              \"#1abc9c\",\n              \"#9b59b6\",\n              \"#d35400\",\n              \"#34495e\",\n              \"#f1c40f\",\n              \"#3498db\",\n              \"#c0392b\",\n              \"#16a085\",\n              \"#f39c12\",\n              \"#2c3e50\",\n              \"#e74c3c\",\n            ],\n            data: [90, 90],\n          },\n        ],\n      },\n      options: {\n        responsive: true,\n        maintainAspectRatio: false,\n        plugins: {\n          legend: {\n            position: \"bottom\",\n          },\n        },\n      },\n    };\n  },\n  methods: {\n    getUsernames() {\n      for (let i = 0; i < this.usersList.length; i++) {\n        this.usernames.push(this.usersList[i].username);\n      }\n    },\n    getTaskSum() {\n      for (let i = 0; i < this.usersList.length; i++) {\n        this.taskSum.push(\n          this.usersList[i].completed + this.usersList[i].not_completed\n        );\n      }\n    },\n  },\n  computed: {\n    usernames() {\n      return this.usersList.map((user) => user.username);\n    },\n  },\n};\n</script>\n<style>\ncanvas {\n  width: auto;\n  height: auto;\n}\n</style>\n"],"mappings":";AAOA,SAASA,KAAI,IAAKC,OAAO,EAAEC,UAAU,EAAEC,OAAO,EAAEC,MAAK,QAAS,UAAU;AACxE,SAASC,QAAO,QAAS,aAAa;AACtCJ,OAAO,CAACK,QAAQ,CAACJ,UAAU,EAAEC,OAAO,EAAEC,MAAM,CAAC;AAE7C,eAAe;EACbG,IAAI,EAAE,eAAe;EACrBC,UAAU,EAAE;IACVH;EACF,CAAC;EACDI,KAAK,EAAE;IACLC,SAAS,EAAEC;EACb,CAAC;EACDC,IAA