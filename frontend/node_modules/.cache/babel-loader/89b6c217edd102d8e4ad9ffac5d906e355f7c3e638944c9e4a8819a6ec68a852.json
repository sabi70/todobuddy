plate>\n<script>\nimport axios from \"axios\";\nimport { mapState } from \"vuex\";\n\nexport default {\n  name: \"HomeView\",\n  data() {\n    return {\n      TASK_API_URL: \"http://192.168.1.115:8000/tasks/\",\n      isValid: false,\n      credentials: btoa(\n        this.userCredentials.user + \":\" + this.userCredentials.password\n      ),\n      basicAuth: \"Basic \" + this.credentials,\n      task: {\n        title: \"\",\n      },\n    };\n  },\n  methods: {\n    createTask() {\n      try {\n        axios.get(this.TASK_API_URL, {\n          headers: { Authorization: +this.basicAuth },\n        });\n      } catch (err) {\n        alert(90);\n      }\n    },\n  },\n  computed: {\n    ...mapState([\"userCredentials\"]),\n  },\n};\n</script>\n<style scoped>\n#task-creation-form {\n  display: grid;\n}\n</style>\n"],"mappings":";AA6BA,OAAOA,KAAI,MAAO,OAAO;AACzB,SAASC,QAAO,QAAS,MAAM;AAE/B,eAAe;EACbC,IAAI,EAAE,UAAU;EAChBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,YAAY,EAAE,kCAAkC;MAChDC,OAAO,EAAE,KAAK;MACdC,WAAW,EAAEC,IAAI,CACf,IAAI,CAACC,eAAe,CAACC,IAAG,GAAI,GAAE,GAAI,IAAI,CAACD,eAAe,CAACE,QACzD,CAAC;MACDC,SAAS,EAAE,QAAO,GAAI,IAAI,CAACL,WAAW;MACtCM,IAAI,EAAE;QACJC,KAAK,EAAE;MACT;IACF,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,UAAUA,CAAA,EAAG;MACX,IAAI;QACFf,KAAK,CAACgB,GAAG,CAAC,IAAI,CAACZ,YAAY,EAAE;UAC3Ba,OAAO,EAAE;YAAEC,aAAa,EAAE,CAAC,IAAI,CAACP;UAAU;QAC5C,CAAC,CAAC;MACJ,EAAE,OAAOQ,GAAG,EAAE;QACZC,KAAK,CAAC,EAAE,CAAC;MACX;IACF;EACF,CAAC;EACDC,QAAQ,EAAE;IACR,GAAGpB,QAAQ,CAAC,CAAC,iBAAiB,CAAC;EACjC;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}