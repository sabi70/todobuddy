{"ast":null,"code":"import { useTheme } from \"vuetify\";\nimport NavDrawer from \"@/components/NavDrawer.vue\";\nimport { mapActions, mapState } from \"vuex\";\nexport default {\n  name: \"AppBar\",\n  components: {\n    NavDrawer\n  },\n  data() {\n    return {\n      theme: useTheme(),\n      darkMode: false\n    };\n  },\n  methods: {\n    kol() {\n      this.$store.commit(\"updateUser\");\n    },\n    changeTheme() {\n      this.darkMode = !this.darkMode;\n      this.theme.global.name = this.darkMode ? \"darkTheme\" : \"lightTheme\";\n    },\n    openDrawer() {\n      this.$refs.navDrawer.openDrawer();\n    }\n  },\n  computed: {\n    ...mapState([\"isAuthenticated\", \"user\"]),\n    ...mapActions([\"updateUser\"])\n  }\n};","map":{"version":3,"names":["useTheme","NavDrawer","mapActions","mapState","name","components","data","theme","darkMode","methods","kol","$store","commit","changeTheme","global","openDrawer","$refs","navDrawer","computed"],"sources":["/home/sabi/Desktop/ToDo/frontend/todo/src/components/AppBar.vue"],"sourcesContent":["<template>\n  <v-app-bar :elevation=\"2\">\n    <template v-slot:prepend>\n      <v-app-bar-nav-icon\n        @click=\"openDrawer\"\n        icon=\"mdi-menu\"\n      ></v-app-bar-nav-icon>\n    </template>\n    <v-app-bar-title class=\"text-center\">\n      <v-btn @click=\"kol\">Click</v-btn>\n      <p v-if=\"isAuthenticated\">lolololo</p>\n    </v-app-bar-title>\n    <template v-slot:append>\n      <v-btn\n        :icon=\"darkMode ? 'mdi-weather-night' : 'mdi-white-balance-sunny'\"\n        :color=\"darkMode ? 'primary' : 'warning'\"\n        @click=\"changeTheme\"\n      ></v-btn>\n    </template>\n  </v-app-bar>\n  <NavDrawer ref=\"navDrawer\"></NavDrawer>\n</template>\n<script>\nimport { useTheme } from \"vuetify\";\nimport NavDrawer from \"@/components/NavDrawer.vue\";\nimport { mapActions, mapState } from \"vuex\";\n\nexport default {\n  name: \"AppBar\",\n  components: {\n    NavDrawer,\n  },\n  data() {\n    return {\n      theme: useTheme(),\n      darkMode: false,\n    };\n  },\n  methods: {\n    kol() {\n      this.$store.commit(\"updateUser\");\n    },\n    changeTheme() {\n      this.darkMode = !this.darkMode;\n      this.theme.global.name = this.darkMode ? \"darkTheme\" : \"lightTheme\";\n    },\n    openDrawer() {\n      this.$refs.navDrawer.openDrawer();\n    },\n  },\n  computed: {\n    ...mapState([\"isAuthenticated\", \"user\"]),\n    ...mapActions([\"updateUser\"]),\n  },\n};\n</script>\n"],"mappings":"AAuBA,SAASA,QAAO,QAAS,SAAS;AAClC,OAAOC,SAAQ,MAAO,4BAA4B;AAClD,SAASC,UAAU,EAAEC,QAAO,QAAS,MAAM;AAE3C,eAAe;EACbC,IAAI,EAAE,QAAQ;EACdC,UAAU,EAAE;IACVJ;EACF,CAAC;EACDK,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAEP,QAAQ,CAAC,CAAC;MACjBQ,QAAQ,EAAE;IACZ,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,GAAGA,CAAA,EAAG;MACJ,IAAI,CAACC,MAAM,CAACC,MAAM,CAAC,YAAY,CAAC;IAClC,CAAC;IACDC,WAAWA,CAAA,EAAG;MACZ,IAAI,CAACL,QAAO,GAAI,CAAC,IAAI,CAACA,QAAQ;MAC9B,IAAI,CAACD,KAAK,CAACO,MAAM,CAACV,IAAG,GAAI,IAAI,CAACI,QAAO,GAAI,WAAU,GAAI,YAAY;IACrE,CAAC;IACDO,UAAUA,CAAA,EAAG;MACX,IAAI,CAACC,KAAK,CAACC,SAAS,CAACF,UAAU,CAAC,CAAC;IACnC;EACF,CAAC;EACDG,QAAQ,EAAE;IACR,GAAGf,QAAQ,CAAC,CAAC,iBAAiB,EAAE,MAAM,CAAC,CAAC;IACxC,GAAGD,UAAU,CAAC,CAAC,YAAY,CAAC;EAC9B;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}