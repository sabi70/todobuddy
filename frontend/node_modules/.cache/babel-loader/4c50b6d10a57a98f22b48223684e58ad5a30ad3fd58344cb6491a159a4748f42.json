{"ast":null,"code":"import axios from \"axios\";\nimport BarChart from \"@/components/BarChart.vue\";\nimport DoughnutChart from \"@/components/DoughnutChart.vue\";\nexport default {\n  name: \"StatisticView\",\n  components: {\n    BarChart,\n    DoughnutChart\n  },\n  data() {\n    return {\n      USERS_LIST_URL: \"http://192.168.1.115:8000/auth/users/all/\",\n      usersList: null,\n      usernames: [],\n      taskSum: []\n    };\n  },\n  methods: {\n    async getUsers() {\n      try {\n        const response = await axios.get(this.USERS_LIST_URL);\n        if (response.status == 200) {\n          const apiData = response.data;\n          this.usersList = apiData;\n          this.usernames = apiData.map(item => item.username);\n        }\n      } catch (err) {\n        console.log(err);\n      }\n    }\n  },\n  async mounted() {\n    await this.getUsers();\n  }\n};","map":{"version":3,"names":["axios","BarChart","DoughnutChart","name","components","data","USERS_LIST_URL","usersList","usernames","taskSum","methods","getUsers","response","get","status","apiData","map","item","username","err","console","log","mounted"],"sources":["/home/sabi/Desktop/ToDo/frontend/todo/src/views/StatisticView.vue"],"sourcesContent":["<template>\n  <div v-for=\"item in usersList\" :key=\"item.pk\">\n    <div v-if=\"item.username == $route.params.username\">\n      <BarChart\n        :completed=\"item.completed\"\n        :notCompleted=\"item.not_completed\"\n        :username=\"item.username\"\n      ></BarChart>\n    </div>\n  </div>\n  <div v-if=\"usersList\">\n    <DoughnutChart\n      :usernames=\"usersList.map((item) => item.username)\"\n      :taskSum=\"\n        usersList.map((item) =>\n          item.completed + item.not_completed == 0\n            ? 1\n            : item.completed + item.not_completed\n        )\n      \"\n    ></DoughnutChart>\n  </div>\n</template>\n<script>\nimport axios from \"axios\";\nimport BarChart from \"@/components/BarChart.vue\";\nimport DoughnutChart from \"@/components/DoughnutChart.vue\";\n\nexport default {\n  name: \"StatisticView\",\n  components: {\n    BarChart,\n    DoughnutChart,\n  },\n  data() {\n    return {\n      USERS_LIST_URL: \"http://192.168.1.115:8000/auth/users/all/\",\n      usersList: null,\n      usernames: [],\n      taskSum: [],\n    };\n  },\n  methods: {\n    async getUsers() {\n      try {\n        const response = await axios.get(this.USERS_LIST_URL);\n        if (response.status == 200) {\n          const apiData = response.data;\n          this.usersList = apiData;\n          this.usernames = apiData.map((item) => item.username);\n        }\n      } catch (err) {\n        console.log(err);\n      }\n    },\n  },\n  async mounted() {\n    await this.getUsers();\n  },\n};\n</script>\n<style></style>\n"],"mappings":"AAwBA,OAAOA,KAAI,MAAO,OAAO;AACzB,OAAOC,QAAO,MAAO,2BAA2B;AAChD,OAAOC,aAAY,MAAO,gCAAgC;AAE1D,eAAe;EACbC,IAAI,EAAE,eAAe;EACrBC,UAAU,EAAE;IACVH,QAAQ;IACRC;EACF,CAAC;EACDG,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,cAAc,EAAE,2CAA2C;MAC3DC,SAAS,EAAE,IAAI;MACfC,SAAS,EAAE,EAAE;MACbC,OAAO,EAAE;IACX,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,QAAQA,CAAA,EAAG;MACf,IAAI;QACF,MAAMC,QAAO,GAAI,MAAMZ,KAAK,CAACa,GAAG,CAAC,IAAI,CAACP,cAAc,CAAC;QACrD,IAAIM,QAAQ,CAACE,MAAK,IAAK,GAAG,EAAE;UAC1B,MAAMC,OAAM,GAAIH,QAAQ,CAACP,IAAI;UAC7B,IAAI,CAACE,SAAQ,GAAIQ,OAAO;UACxB,IAAI,CAACP,SAAQ,GAAIO,OAAO,CAACC,GAAG,CAAEC,IAAI,IAAKA,IAAI,CAACC,QAAQ,CAAC;QACvD;MACF,EAAE,OAAOC,GAAG,EAAE;QACZC,OAAO,CAACC,GAAG,CAACF,GAAG,CAAC;MAClB;IACF;EACF,CAAC;EACD,MAAMG,OAAOA,CAAA,EAAG;IACd,MAAM,IAAI,CAACX,QAAQ,CAAC,CAAC;EACvB;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}