{"ast":null,"code":"import { useTheme } from \"vuetify\";\nimport NavDrawer from \"@/components/NavDrawer.vue\";\nimport { mapState } from \"vuex\";\nimport store from \"@/store\";\nexport default {\n  name: \"AppBar\",\n  components: {\n    NavDrawer\n  },\n  data() {\n    return {\n      theme: useTheme(),\n      darkMode: false\n    };\n  },\n  methods: {\n    setUser() {\n      const usernameCookies = this.$cookies.get(\"user\");\n      store.dispatch(\"updateUser\", {\n        user: usernameCookies,\n        isAuthenticated: true\n      });\n    },\n    changeTheme() {\n      this.darkMode = !this.darkMode;\n      this.theme.global.name = this.darkMode ? \"darkTheme\" : \"lightTheme\";\n    },\n    openDrawer() {\n      this.$refs.navDrawer.openDrawer();\n    }\n  },\n  computed: {\n    ...mapState([\"isAuthenticated\", \"user\"])\n  }\n};","map":{"version":3,"names":["useTheme","NavDrawer","mapState","store","name","components","data","theme","darkMode","methods","setUser","usernameCookies","$cookies","get","dispatch","user","isAuthenticated","changeTheme","global","openDrawer","$refs","navDrawer","computed"],"sources":["/home/sabi/Desktop/ToDo/frontend/todo/src/components/AppBar.vue"],"sourcesContent":["<template>\n  <v-app-bar :elevation=\"2\">\n    <template v-slot:prepend>\n      <v-app-bar-nav-icon\n        @click=\"openDrawer\"\n        icon=\"mdi-menu\"\n      ></v-app-bar-nav-icon>\n    </template>\n    <v-app-bar-title class=\"text-center\">\n      <v-btn @click=\"setUser\">click</v-btn>\n      <p>{{ user }}</p>\n    </v-app-bar-title>\n    <template v-slot:append>\n      <v-btn\n        :icon=\"darkMode ? 'mdi-weather-night' : 'mdi-white-balance-sunny'\"\n        :color=\"darkMode ? 'primary' : 'warning'\"\n        @click=\"changeTheme\"\n      ></v-btn>\n    </template>\n  </v-app-bar>\n  <NavDrawer ref=\"navDrawer\"></NavDrawer>\n</template>\n<script>\nimport { useTheme } from \"vuetify\";\nimport NavDrawer from \"@/components/NavDrawer.vue\";\nimp