{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from \"axios\";\nimport BarChart from \"@/components/BarChart.vue\";\nimport DoughnutChart from \"@/components/DoughnutChart.vue\";\nexport default {\n  name: \"StatisticView\",\n  components: {\n    BarChart,\n    DoughnutChart\n  },\n  data() {\n    return {\n      USERS_LIST_URL: \"http://192.168.1.115:8000/auth/users/all/\",\n      usersList: null,\n      usernames: [],\n      taskSum: [],\n      userStats: {\n        completed: null,\n        notCompleted: null\n      }\n    };\n  },\n  methods: {\n    getUsers() {\n      try {\n        axios.get(this.USERS_LIST_URL).then(response => {\n          if (response.status == 200) {\n            this.usersList = response.data;\n            for (let i = 0; i < this.usersList.length; i++) {\n              this.usernames.push(this.usersList[i].username);\n              const mySum = this.usersList[i].completed + this.usersList[i].not_completed;\n              if (mySum != 0) {\n                this.taskSum.push(mySum);\n              }\n              const childComponent = this.$refs.doughnutComp;\n              childComponent.changeTaskSum(this.taskSum);\n            }\n          }\n        });\n      } catch (err) {\n        alert(\"90\");\n      }\n    }\n  },\n  mounted() {\n    this.getUsers();\n  }\n};","map":{"version":3,"names":["axios","BarChart","DoughnutChart","name","components","data","USERS_LIST_URL","usersList","usernames","taskSum","userStats","completed","notCompleted","methods","getUsers","get","then","response","status","i","length","push","username","mySum","not_completed","childComponent","$refs","doughnutComp","changeTaskSum","err","alert","mounted"],"sources":["/home/sabi/Desktop/ToDo/frontend/todo/src/views/StatisticView.vue"],"sourcesContent":["<template>\n  <div v-for=\"item in usersList\" :key=\"item.pk\">\n    <div v-if=\"item.username == $route.params.username\">\n      <BarChart\n        :completed=\"item.completed\"\n        :notCompleted=\"item.not_completed\"\n        :username=\"item.username\"\n      ></BarChart>\n    </div>\n  </div>\n  <div>\n    <DoughnutChart :usernames=\"usernames\" ref=\"doughnutComp\"></DoughnutChart>\n  </div>\n</template>\n<script>\nimport axios from \"axios\";\nimport BarChart from \"@/components/BarChart.vue\";\nimport DoughnutChart from \"@/components/DoughnutChart.vue\";\n\nexport default {\n  name: \"StatisticView\",\n  components: {\n    BarChart,\n    DoughnutChart,\n  },\n  data() {\n    return {\n      USERS_LIST_URL: \"http://192.168.1.115:8000/auth/users/all/\",\n      usersList: null,\n      usernames: [],\n      taskSum: [],\n      userStats: {\n        completed: null,\n        notCompleted: null,\n      },\n    };\n  },\n  methods: {\n    getUsers() {\n      try {\n        axios.get(this.USERS_LIST_URL).then((response) => {\n          if (response.status == 200) {\n            this.usersList = response.data;\n            for (let i = 0; i < this.usersList.length; i++) {\n              this.usernames.push(this.usersList[i].username);\n              const mySum = this.usersList[i].completed + this.usersList[i].not_completed            \n              if (mySum != 0) {\n                this.taskSum.push(mySum);\n              }\n              const childComponent = this.$refs.doughnutComp;\n              childComponent.changeTaskSum(this.taskSum);\n            }\n          }\n        });\n      } catch (err) {\n        alert(\"90\");\n      }\n    },\n  },\n  mounted() {\n    this.getUsers();\n  },\n};\n</script>\n<style></style>\n"],"mappings":";AAeA,OAAOA,KAAI,MAAO,OAAO;AACzB,OAAOC,QAAO,MAAO,2BAA2B;AAChD,OAAOC,aAAY,MAAO,gCAAgC;AAE1D,eAAe;EACbC,IAAI,EAAE,eAAe;EACrBC,UAAU,EAAE;IACVH,QAAQ;IACRC;EACF,CAAC;EACDG,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,cAAc,EAAE,2CAA2C;MAC3DC,SAAS,EAAE,IAAI;MACfC,SAAS,EAAE,EAAE;MACbC,OAAO,EAAE,EAAE;MACXC,SAAS,EAAE;QACTC,SAAS,EAAE,IAAI;QACfC,YAAY,EAAE;MAChB;IACF,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,QAAQA,CAAA,EA