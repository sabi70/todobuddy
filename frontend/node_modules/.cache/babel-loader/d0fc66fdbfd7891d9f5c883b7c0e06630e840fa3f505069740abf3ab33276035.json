\n        { title: \"About\", url: \"about\", icon: \"mdi-information-outline\" },\n      ],\n      users: [\n        { name: \"Mico\", url: \"/\" },\n        { name: \"Stella\", url: \"/\" },\n        { name: \"Ben\", url: \"/\" },\n        { name: \"Mico\", url: \"/\" },\n        { name: \"Stella\", url: \"/\" },\n      ],\n    };\n  },\n  methods: {\n    changeTheme() {\n      this.darkMode = !this.darkMode;\n      this.theme.global.name = this.darkMode ? \"darkMode\" : \"lightMode\";\n    },\n    emptyFunction () {},\n    updateSidebar() {\n      store.dispatch(\"updateDrawer\", {\n        drawer: !this.drawer,\n      });\n    },\n  },\n  computed: {\n    ...mapState([\"drawer\"]),\n    windowWidth() {\n      return this.$vuetify.display.width;\n    },\n  },\n};\n</script>\n<style scoped>\n@media screen and (max-width: 600px) {\n  #sidebar {\n    position: fixed;\n    top: 64px;\n    width: 100%;\n  }\n}\n@media screen and (min-width: 992px) {\n  #sidebar {\n    position: fixed;\n    top: 64px;\n    width: 250px;\n  }\n}\n</style>\n"],"mappings":"AA6CA,SAASA,QAAO,QAAS,SAAS;AAClC,SAASC,QAAO,QAAS,MAAM;AAC/B,OAAOC,KAAI,MAAO,SAAS;AAE3B,eAAe;EACbC,IAAI,EAAE,qBAAqB;EAC3BC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,OAAO,EAAE,IAAI;MACbC,KAAK,EAAEN,QAAQ,CAAC,CAAC;MACjBO,QAAQ,EAAE,KAAK;MACfC,YAAY,EAAE,CACZ;QAAEC,KAAK,EAAE,MAAM;QAAEC,GAAG,EAAE,GAAG;QAAEC,IAAI,EAAE;MAAmB,CAAC,EACrD;QAAEF,KAAK,EAAE,UAAU;QAAEC,GAAG,EAAE,UAAU;QAAEC,IAAI,EAAE;MAAkB,CAAC,EAC/D;QAAEF,KAAK,EAAE,UAAU;QAAEC,GAAG,EAAE,UAAU;QAAEC,IAAI,EAAE;MAAmB,CAAC,EAChE;QAAEF,KAAK,EAAE,OAAO;QAAEC,GAAG,EAAE,OAAO;QAAEC,IAAI,EAAE;MAAY,CAAC,EACnD;QAAEF,KAAK,EAAE,OAAO;QAAEC,GAAG,EAAE,OAAO;QAAEC,IAAI,EAAE;MAA0B,CAAC,CAClE;MACDC,KAAK,EAAE,CACL;QAAET,IAAI,EAAE,MAAM;QAAEO,GAAG,EAAE;MAAI,CAAC,EAC1B;QAAEP,IAAI,EAAE,QAAQ;QAAEO,GAAG,EAAE;MAAI,CAAC,EAC5B;QAAEP,IAAI,EAAE,KAAK;QAAEO,GAAG,EAAE;MAAI,CAAC,EACzB;QAAEP,IAAI,EAAE,MAAM;QAAEO,GAAG,EAAE;MAAI,CAAC,EAC1B;QAAEP,IAAI,EAAE,QAAQ;QAAEO,GAAG,EAAE;MAAI,CAAC;IAEhC,CAAC;EACH,CAAC;EACDG,OAAO,EAA