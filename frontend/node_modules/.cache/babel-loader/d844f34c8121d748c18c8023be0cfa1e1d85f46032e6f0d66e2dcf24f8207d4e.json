{"ast":null,"code":"import HeaderComponent from \"@/components/Header.vue\";\nimport SideBar from \"@/components/SideBar.vue\";\nimport SectionComponent from \"@/components/Section.vue\";\nimport ContentComponent from \"@/components/Content.vue\";\nimport FooterComponent from \"@/components/Footer.vue\";\nexport default {\n  name: \"App\",\n  components: {\n    HeaderComponent,\n    SideBar,\n    SectionComponent,\n    ContentComponent,\n    FooterComponent\n  }\n};","map":{"version":3,"names":["HeaderComponent","SideBar","SectionComponent","ContentComponent","FooterComponent","name","components"],"sources":["/home/sabi/Desktop/ToDo/frontend/todo/src/App.vue"],"sourcesContent":["<template>\n  <v-app>\n    <div class=\"container\">\n      <nav class=\"elevation-2\"><HeaderComponent></HeaderComponent></nav>\n      <aside><SideBar></SideBar></aside>\n      <main><router-view /></main>\n      <div class=\"section\"><SectionComponent></SectionComponent></div>\n      <div class=\"content\"><ContentComponent></ContentComponent></div>\n      <footer><FooterComponent></FooterComponent></footer>\n    </div>\n  </v-app>\n</template>\n\n<script>\nimport HeaderComponent from \"@/components/Header.vue\";\nimport SideBar from \"@/components/SideBar.vue\";\nimport SectionComponent from \"@/components/Section.vue\";\nimport ContentComponent from \"@/components/Content.vue\";\nimport FooterComponent from \"@/components/Footer.vue\";\n\nexport default {\n  name: \"App\",\n  components: {\n    HeaderComponent,\n    SideBar,\n    SectionComponent,\n    ContentComponent,\n    FooterComponent,\n  },\n};\n</script>\n<style>\nbody {\n  text-align: center;\n}\nhtml {\n  scrollbar-width: none;\n}\nnav {\n  grid-area: header;\n  top: 0;\n  position: sticky;\n  background: white;\n  z-index: 1;\n}\naside {\n  grid-area: sidebar;\n  display: none;\n  background: #ddbea9;\n}\nmain {\n  grid-area: main;\n  background: #ffe8d6;\n  top: 1;\n}\n.section {\n  grid-area: section;\n  background: #b7b7a4;\n}\n.content {\n  grid-area: content;\n  background: #a5a58d;\n}\nfooter {\n  grid-area: footer;\n  background: #6b705c;\n}\n.container {\n  display: grid;\n  min-height: 100vh;\n  grid-gap: 0px;\n  grid-template-columns: 1fr;\n  grid-template-rows: auto 1500px auto auto auto;\n  grid-template-areas:\n    \"header\"\n    \"main\"\n    \"section\"\n    \"content\"\n    \"footer\";\n}\n/* Media query for screen width  till 600px */\n@media only screen and (max-width: 600px) {\n  main {\n    background: green;\n  }\n}\n/* Media query for screen width up to 601px */\n@media only screen and (min-width: 601px) {\n  main {\n    background: white;\n  }\n}\n/* Media query for screen width up to 768px */\n@media only screen and (min-width: 768px) {\n  main {\n    background: purple;\n  }\n  aside {\n    display: block;\n  }\n  .container {\n    grid-template-columns: 250px 1fr;\n    grid-template-rows: auto 1fr auto auto auto;\n    grid-template-areas:\n      \"header header\"\n      \"sidebar main\"\n      \"sidebar section\"\n      \"sidebar content\"\n      \"sidebar footer\";\n  }\n}\n/* Media query for screen width up to 992px */\n@media only screen and (min-width: 992px) {\n  main {\n    background: orange;\n  }\n  .container {\n    grid-template-columns: 275px 1fr 1fr;\n    grid-template-rows: auto 1fr auto auto;\n    grid-template-areas:\n      \"header header header\"\n      \"sidebar main main\"\n      \"sidebar section content\"\n      \"sidebar footer footer\";\n  }\n}\n/* Media query for screen width up to 1200px */\n@media only screen and (min-width: 1200px) {\n  main {\n    background: grey;\n  }\n}\n</style>\n"],"mappings":"AAcA,OAAOA,eAAc,MAAO,yBAAyB;AACrD,OAAOC,OAAM,MAAO,0BAA0B;AAC9C,OAAOC,gBAAe,MAAO,0BAA0B;AACvD,OAAOC,gBAAe,MAAO,0BAA0B;AACvD,OAAOC,eAAc,MAAO,yBAAyB;AAErD,eAAe;EACbC,IAAI,EAAE,KAAK;EACXC,UAAU,EAAE;IACVN,eAAe;IACfC,OAAO;IACPC,gBAAgB;IAChBC,gBAAgB;IAChBC;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies