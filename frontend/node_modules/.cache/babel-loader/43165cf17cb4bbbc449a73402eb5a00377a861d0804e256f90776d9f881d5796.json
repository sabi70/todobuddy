{"ast":null,"code":"import axios from \"axios\";\nimport BarChart from \"@/components/BarChart.vue\";\nimport DoughnutChart from \"@/components/DoughnutChart.vue\";\nexport default {\n  name: \"StatisticView\",\n  components: {\n    BarChart,\n    DoughnutChart\n  },\n  data() {\n    return {\n      USERS_LIST_URL: \"http://192.168.1.115:8000/auth/users/all/\",\n      usersList: null,\n      usernames: [],\n      taskSum: [],\n      userStats: {\n        completed: null,\n        notCompleted: null\n      }\n    };\n  },\n  methods: {\n    getUsers() {\n      try {\n        axios.get(this.USERS_LIST_URL).then(response => {\n          if (response.status == 200) {\n            this.usersList = response.data;\n            this.transformData();\n          }\n        });\n      } catch (err) {\n        alert(\"90\");\n      }\n    },\n    transformData() {\n      console.log(90);\n    }\n  },\n  mounted() {\n    this.getUsers();\n  }\n};","map":{"version":3,"names":["axios","BarChart","DoughnutChart","name","components","data","USERS_LIST_URL","usersList","usernames","taskSum","userStats","completed","notCompleted","methods","getUsers","get","then","response","status","transformData","err","alert","console","log","mounted"],"sources":["/home/sabi/Desktop/ToDo/frontend/todo/src/views/StatisticView.vue"],"sourcesContent":["<template>\n  <div v-for=\"item in usersList\" :key=\"item.pk\">\n    <div v-if=\"item.username == $route.params.username\">\n      <BarChart\n        :completed=\"item.completed\"\n        :notCompleted=\"item.not_completed\"\n        :username=\"item.username\"\n      ></BarChart>\n    </div>\n  </div>\n  <div>\n    <DoughnutChart :usernames=\"usernames\" :taskSum=\"taskSum\"></DoughnutChart>\n  </div>\n</template>\n<script>\nimport axios from \"axios\";\nimport BarChart from \"@/components/BarChart.vue\";\nimport DoughnutChart from \"@/components/DoughnutChart.vue\";\n\nexport default {\n  name: \"StatisticView\",\n  components: {\n    BarChart,\n    DoughnutChart,\n  },\n  data() {\n    return {\n      USERS_LIST_URL: \"http://192.168.1.115:8000/auth/users/all/\",\n      usersList: null,\n      usernames: [],\n      taskSum: [],\n      userStats: {\n        completed: null,\n        notCompleted: null,\n      },\n    };\n  },\n  methods: {\n    getUsers() {\n      try {\n        axios.get(this.USERS_LIST_URL).then((response) => {\n          if (response.status == 200) {\n            this.usersList = response.data;\n            this.transformData();\n          }\n        });\n      } catch (err) {\n        alert(\"90\");\n      }\n    },\n    transformData() {\n      console.log(90);\n    },\n  },\n  mounted() {\n    this.getUsers();\n  },\n};\n</script>\n<style></style>\n"],"mappings":"AAeA,OAAOA,KAAI,MAAO,OAAO;AACzB,OAAOC,QAAO,MAAO,2BAA2B;AAChD,OAAOC,aAAY,MAAO,gCAAgC;AAE1D,eAAe;EACbC,IAAI,EAAE,eAAe;EACrBC,UAAU,EAAE;IACVH,QAAQ;IACRC;EACF,CAAC;EACDG,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,cAAc,EAAE,2CAA2C;MAC3DC,SAAS,EAAE,IAAI;MACfC,SAAS,EAAE,EAAE;MACbC,OAAO,EAAE,EAAE;MACXC,SAAS,EAAE;QACTC,SAAS,EAAE,IAAI;QACfC,YAAY,EAAE;MAChB;IACF,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,QAAQA,CAAA,EAAG;MACT,IAAI;QACFd,KAAK,CAACe,GAAG,CAAC,IAAI,CAACT,cAAc,CAAC,CAACU,IAAI,CAAEC,QAAQ,IAAK;UAChD,IAAIA,QAAQ,CAACC,MAAK,IAAK,GAAG,EAAE;YAC1B,IAAI,CAACX,SAAQ,GAAIU,QAAQ,CAACZ,IAAI;YAC9B,IAAI,CAACc,aAAa,CAAC,CAAC;UACtB;QACF,CAAC,CAAC;MACJ,EAAE,OAAOC,GAAG,EAAE;QACZC,KAAK,CAAC,IAAI,CAAC;MACb;IACF,CAAC;IACDF,aAAaA,CAAA,EAAG;MACdG,OAAO,CAACC,GAAG,CAAC,EAAE,CAAC;IACjB;EACF,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACV,QAAQ,CAAC,CAAC;EACjB;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}