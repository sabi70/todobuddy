    this.snackbar = true;\n              this.getTask();\n            } else {\n              alert(\"00\");\n            }\n          });\n      } catch (err) {\n        alert(\"60\");\n      }\n    },\n    async getTask() {\n      try {\n        const response = await axios.get(this.TASK_API_URL, {\n          headers: {\n            Authorization:\n              \"Basic \" +\n              btoa(\n                this.userCredentials.user +\n                  \":\" +\n                  this.userCredentials.password\n              ),\n          },\n        });\n        this.taskList = response.data;\n      } catch {\n        alert(\"90\");\n      }\n    },\n    updateUserStatistic(status) {\n      try {\n        axios\n          .put(\n            this.STATISTIC_UPDATING_URL,\n            {\n              completed: status,\n              not_completed: !status,\n            },\n            {\n              headers: {\n                Authorization:\n                  \"Basic \" +\n                  btoa(\n                    this.userCredentials.user +\n                      \":\" +\n                      this.userCredentials.password\n                  ),\n              },\n            }\n          )\n          .then((response) => {\n            if (response.status == 200) {\n              /* Hello World! */\n            }\n          });\n      } catch (err) {\n        alert(\"90\");\n      }\n    },\n    deleteTask(pk, status) {\n      try {\n        axios\n          .delete(this.TASK_API_URL + String(pk) + \"/\", {\n            headers: {\n              Authorization:\n                \"Basic \" +\n                btoa(\n                  this.userCredentials.user +\n                    \":\" +\n                    this.userCredentials.password\n                ),\n            },\n          })\n          .then((response) => {\n            if (response.status == 204) {\n              this.updateUserStatistic(status);\n              this.getTask();\n              this.snackbar = true;\n            }\n          });\n      } catch (err) {\n        alert(\"90\");\n      }\n    },\n  },\n  computed: {\n    userCredentials() {\n      return this.$store.state.userCredentials;\n    },\n  },\n  mounted() {\n    this.getTask();\n  },\n};\n</script>\n<style scoped>\n#task-container {\n  display: flex;\n  flex-wrap: wrap;\n  align-content: flex-start;\n  gap: 1em;\n}\n@media screen and (max-width: 992px) {\n  #task-container {\n    display: inline-flex;\n  }\n  #task-card {\n    width: 100%;\n  }\n}\n</style>\n"],"mappings":";AA+DA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAI,EAAE,UAAU;EAChBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,YAAY,EAAE,kCAAkC;MAChDC,sBAAsB,EAAE,gDAAgD;MACxEC,QAAQ,EAAE,KAAK;MACfC,OAAO,EAAE,KAAK;MACdC,QAAQ,EAAE,EAAE;MACZC,IAAI,EAAE;QACJC,KAAK,EAAE;MACT;IACF,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,UAAUA,CAAA,EAAG;MACX,IAAI;QACFX,KAAI,CACDY,IAAI,CACH,IAAI,CAACT,YAAY,EACjB;UACEM,KAAK,EAAE,IAAI,CAACD,IAAI,CAACC;QACnB,CAAC,EACD;UACEI,OAAO,EAAE;YACPC,aAAa,EACX,QAAO,GACPC,IAAI,CACF,IAAI,CAACC,eAAe,CAACC,IAAG,GACtB,GAAE,GACF,IAAI,CAACD,eAAe,CAACE,QACzB;UACJ;QACF,CACF,EACCC,IAAI,CAAEC,QAAQ,IAAK;UAClB,IAAIA,QAAQ,CAACC,MAAK,IAAK,GAAG,EAAE;YAC1B,IAAI,CAACb,IAAI,CAACC,KAAI,GAAI,EAAE;YACpB,IAAI,CAACJ,QAAO,GAAI,IAAI;YACpB,IAAI,CAACiB,OAAO,CAAC,CAAC;UAChB,OAAO;YACLC,KAAK,CAAC,IAAI,CAAC;UACb;QACF,CAAC,CAAC;MACN,EAAE,OAAOC,GAAG,EAAE;QACZD,KAAK,CAAC,IAAI,CAAC;MACb;IACF,CAAC;IACD,MAAMD,OAAOA,CAAA,EAAG;MACd,IAAI;QACF,MAAMF,QAAO,GAAI,MAAMpB,KAAK,CAACyB,GAAG,CAAC,IAAI,CAACtB,YAAY,EAAE;UAClDU,OAAO,EAAE;YACPC,aAAa,EACX,QAAO,GACPC,IAAI,CACF,IAAI,CAACC,eAAe,CAACC,IAAG,GACtB,GAAE,GACF,IAAI,CAACD,eAAe,CAACE,QACzB;UACJ;QACF,CAAC,CAAC;QACF,IAAI,CAACX,QAAO,GAAIa,QAAQ,CAAClB,IAAI;MAC/B,EAAE,MAAM;QACNqB,KAAK,CAAC,IAAI,CAAC;MACb;IACF,CAAC;IACDG,mBAAmBA,CAACL,MAAM,EAAE;MAC1B,IAAI;QACFrB,KAAI,CACD2B,GAAG,CACF,IAAI,CAACvB,sBAAsB,EAC3B;UACEwB,SAAS,EAAEP,MAAM;UACjBQ,aAAa,EAAE,CAACR;QAClB,CAAC,EACD;UACER,OAAO,EAAE;YACPC,aAAa,EACX,QAAO,GACPC,IAAI,CACF,IAAI,CAACC,eAAe,C